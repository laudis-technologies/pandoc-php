{
  "name": "laudis/pandoc",
  "description": "simple wrapper for easy pandoc handling",
  "type": "library",
  "keywords": [
    "pandoc",
    "pdf",
    "conversion",
    "php",
    "haskell"
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.13",
    "friendsofphp/php-cs-fixer": "^3.3.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "ghlen",
      "email": "ghlen@pm.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "Laudis\\Pandoc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Laudis\\Pandoc\\Tests\\": "tests/"
    }
  },
  "require": {
    "youngsource/typed-enum": "^1.1",
    "symfony/process": "^5.0",
    "psalm/plugin-phpunit": "^0.16"
  }
}
